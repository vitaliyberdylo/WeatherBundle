{
    "name": "vitber/weather-bundle",
    "description": "Bundle to obtain current weather for specific cities",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "vitaliyberdylo"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "8.2.*",
        "symfony/framework-bundle": "^7.0",
        "symfony/http-client": "7.0.*",
        "symfony/serializer": "7.0.*",
        "symfony/property-info": "7.0.*",
        "symfony/yaml": "^7.0",
        "symfony/twig-bundle": "7.0.*"
    },
    "autoload": {
        "psr-4": {
            "Vitber\\WeatherBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vitber\\WeatherBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "7.1.x-dev"
    }
}
